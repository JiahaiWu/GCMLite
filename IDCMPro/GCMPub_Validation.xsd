        <xs:element name="light_conditions" type="xs:string" minOccurs="0" />
        <xs:element name="deficiencies" type="xs:string" minOccurs="0" />
        <xs:element name="date_of_accession" type="date-or-empty" minOccurs="0" />
        <xs:element name="form_of_supply" type="xs:string" minOccurs="0" />
        <xs:element name="conditions_of_water_activity" type="xs:string" minOccurs="0" />
        <xs:element name="cell_contents" type="xs:string" minOccurs="0" />
        <xs:element name="phenotype" type="xs:string" minOccurs="0" />
        <xs:element name="deposited_by" type="xs:string" minOccurs="0" />
        <xs:element name="minimum_growth_ph" type="xs:string" minOccurs="0" />
        <xs:element name="cellshape" type="xs:string" minOccurs="0" />
        <xs:element name="maximum_growth_ph" type="xs:string" minOccurs="0" />
        <xs:element name="toxicity_to_other_organisms" type="xs:string" minOccurs="0" />
        <xs:element name="restriction_for_destribute" type="xs:string" minOccurs="0" />
        <xs:element name="maximum_growthtemperature" type="xs:string" minOccurs="0" />
        <xs:element name="hybridization_with_other_strains" type="xs:string" minOccurs="0" />
        <xs:element name="rehydration_fluid" type="xs:string" minOccurs="0" />
        <xs:element name="temperature_of_hybridization" type="xs:string" minOccurs="0" />
        <xs:element name="strainid" type="xs:string" minOccurs="0" />
        <xs:element name="serovar" type="xs:string" minOccurs="0" />
        <xs:element name="latitude" type="xs:string" minOccurs="0" />
