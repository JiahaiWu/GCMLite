        <xs:element name="habitat" type="xs:string" minOccurs="0" />
        <xs:element name="hybrids" type="xs:string" minOccurs="0" />
        <xs:element name="mta" type="xs:string" minOccurs="0" />
        <xs:element name="sexual_behaviour" type="xs:string" minOccurs="0" />
        <xs:element name="identified_by" type="xs:string" minOccurs="0" />
        <xs:element name="sexual_state" type="xs:string" minOccurs="0" />
        <xs:element name="method_of_isolation" type="xs:string" minOccurs="0" />
        <xs:element name="sporeforming" type="xs:string" minOccurs="0" />
        <xs:element name="state" type="xs:string" minOccurs="0" />
        <xs:element name="cellsize" type="xs:string" minOccurs="0" />
        <xs:element name="gc_content_of_dna" type="xs:string" minOccurs="0" />
        <xs:element name="wall_constituents" type="xs:string" minOccurs="0" />
        <xs:element name="anamorph_name" type="xs:string" minOccurs="0" />
        <xs:element name="conditions_for_growth_in_liquid_media" type="xs:string" minOccurs="0" />
        <xs:element name="staining_reactions" type="xs:string" minOccurs="0" />
        <xs:element name="number_of_nuclei" type="xs:string" minOccurs="0" />
        <xs:element name="collected_by" type="xs:string" minOccurs="0" />
        <xs:element name="substrate" type="xs:string" minOccurs="0" />
        <xs:element name="symbiosis" type="xs:string" minOccurs="0" />
        <xs:element name="date_of_identification" type="date-or-empty" minOccurs="0" />
        <xs:element name="former_name" type="xs:string" minOccurs="0" />
        <xs:element name="source_of_isolation" type="xs:string" minOccurs="0" />
        <xs:element name="conditions_for_growth_and_maintenance_on_solid_media" type="xs:string" minOccurs="0" />
        <xs:element name="nitrogen_sources_that_have_been_tested_for_growth" type="xs:string" minOccurs="0" />
        <xs:element name="hybridization_strain_number" type="xs:string" minOccurs="0" />
        <xs:element name="pigment_production_and_autofluorescence" type="xs:string" minOccurs="0" />
        <xs:element name="allergenicity" type="xs:string" minOccurs="0" />
        <xs:element name="percentage" type="xs:string" minOccurs="0" />
        <xs:element name="coenzyme-q_system" type="xs:string" minOccurs="0" />
        <xs:element name="mycoparasitism" type="xs:string" minOccurs="0" />
