<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="GCMPub_Validation"
    targetNamespace="http://tempuri.org/GCMPub_Validation.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/GCMPub_Validation.xsd"
    xmlns:mstns="http://tempuri.org/GCMPub_Validation.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>

  <xs:simpleType name="date-or-empty">
    <xs:union memberTypes="xs:date">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value=""/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:element name="strains" >
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="strain" minOccurs="1" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="strain">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="strain_number" type="xs:string"  />
        <xs:element name="other_collection_numbers" type="xs:string" minOccurs="0" />
        <xs:element name="genus_name" type="xs:string"  />
        <xs:element name="species_epithet" type="xs:string" minOccurs="0" />
        <xs:element name="subspecies_name" type="xs:string" minOccurs="0" />
        <xs:element name="variety_name" type="xs:string" minOccurs="0" />
        <xs:element name="author" type="xs:string" minOccurs="0" />

        <xs:element name="organism_type" >
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Antibody" />
              <xs:enumeration value="Archaea" />
              <xs:enumeration value="Bacteria" />
              <xs:enumeration value="Fungi" />
              <xs:enumeration value="Microalgae" />
              <xs:enumeration value="Phage" />
              <xs:enumeration value="Virus" />
              <xs:enumeration value="Yeast" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="history_of_deposit" type="xs:string" minOccurs="0" />

        <xs:element name="date_of_isolation" minOccurs="0" type="date-or-empty">
        </xs:element>
        <xs:element name="isolated_from" type="xs:string" minOccurs="0" />
        <xs:element name="geographic_origin" type="xs:string" minOccurs="0" />

        <xs:element name="status" minOccurs="0" >
          <xs:simpleType>
            <xs:restriction base="xs:string" >
              <xs:enumeration value="Type" />
              <xs:enumeration value="No" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="type_strain" type="xs:string" minOccurs="0" />
        <xs:element name="optimum_temperature_for_growth" type="xs:string" minOccurs="0" />
        <xs:element name="maximum_temperature_for_growth" type="xs:string" minOccurs="0" />
        <xs:element name="minimum_temperature_for_growth" type="xs:string" minOccurs="0" />
        <xs:element name="medium" type="xs:string" minOccurs="0" />
        <xs:element name="application" type="xs:string" minOccurs="0" />
        <xs:element name="literature" type="xs:string" minOccurs="0" />
        <xs:element name="soursurl" type="xs:string" minOccurs="0" />
        <xs:element name="accession_number" type="xs:string" minOccurs="0" />
        <xs:element name="sequence_type" type="xs:string" minOccurs="0" />
        <xs:element name="sequence_name" type="xs:string" minOccurs="0" />
        <xs:element name="description" type="xs:string" minOccurs="0" />
        <xs:element name="sequence" type="xs:string" minOccurs="0" />
        <xs:element name="length" type="xs:string" minOccurs="0" />
        <xs:element name="primer" type="xs:string" minOccurs="0" />
        <xs:element name="sequencing_method" type="xs:string" minOccurs="0" />
        <xs:element name="sequencing_time" type="xs:string" minOccurs="0" />
        <xs:element name="sequencing_image" type="xs:string" minOccurs="0" />
        <xs:element name="medium_number" type="xs:string" minOccurs="0" />
        <xs:element name="medium_name" type="xs:string" minOccurs="0" />
        <xs:element name="composition" type="xs:string" minOccurs="0" />
        <xs:element name="collection" type="xs:string" minOccurs="0" />
        <xs:element name="heat_resistance" type="xs:string" minOccurs="0" />
        <xs:element name="altitude" type="xs:string" minOccurs="0" />
        <xs:element name="humidity_for_environment" type="xs:string" minOccurs="0" />
        <xs:element name="ethanol_conditions" type="xs:string" minOccurs="0" />
        <xs:element name="tolerances_and_sensitivities" type="xs:string" minOccurs="0" />
        <xs:element name="conditions_for_fruiting_or_sporulation" type="xs:string" minOccurs="0" />
        <xs:element name="ph_for_environment" type="xs:string" minOccurs="0" />
        <xs:element name="biotransformations" type="xs:string" minOccurs="0" />
        <xs:element name="single_compounds_tested" type="xs:string" minOccurs="0" />
        <xs:element name="enzymes_produced" type="xs:string" minOccurs="0" />
        <xs:element name="fine_structure_data" type="xs:string" minOccurs="0" />
        <xs:element name="osmophily_and_xerophily" type="xs:string" minOccurs="0" />
