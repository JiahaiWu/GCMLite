        <xs:element name="minimum_growth_temperatue" type="xs:string" minOccurs="0" />
        <xs:element name="plasmid" type="xs:string" minOccurs="0" />
        <xs:element name="date_of_collection" type="date-or-empty" minOccurs="0" />
        <xs:element name="special_form_name" type="xs:string" minOccurs="0" />
        <xs:element name="decomposition_and_deteriorating_capacities" type="xs:string" minOccurs="0" />
