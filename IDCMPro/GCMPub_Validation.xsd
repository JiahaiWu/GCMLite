        <xs:element name="motile" type="xs:string" minOccurs="0" />
        <xs:element name="name_at_accept" type="xs:string" minOccurs="0" />
        <xs:element name="method_by_which_mutation_was_induced" type="xs:string" minOccurs="0" />
        <xs:element name="optimal_growth_temperatue" type="xs:string" minOccurs="0" />
        <xs:element name="optimal_growth_ph" type="xs:string" minOccurs="0" />
        <xs:element name="collection_method" type="xs:string" minOccurs="0" />
        <xs:element name="race" type="xs:string" minOccurs="0" />
        <xs:element name="remarks_on_restrictions" type="xs:string" minOccurs="0" />
        <xs:element name="type_of_dna_or_rna" type="xs:string" minOccurs="0" />
